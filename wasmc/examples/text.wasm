#meta
name: "Text Section Test"
author: "Kai Tamkun"
version: "1.1"
orcid: "0000-0001-7405-6654"

#text

%data

@message
%stringz "Hello, World!\n"

%type message object
%size message .-message

@probably_four
%8b 4 + (main - main) + (2*2*2+(3-3)) / .
%8b 92
%8b probably_four-message
%4b .-probably_four
%8b probably_four - 1
@from_text2
%8b text2_data + 400

%code

@main
	&message -> $a0
	// Insert some nops
	%4b 0
	%align 8
	%fill 10 0
	%1b 0
	%align 8
	:: strprint
	[from_text2] -> $t0
	<prx $t0>
	:: text2_code
	<halt>

%type main function

@strprint
	[$a0] -> $ma /b
	: _strprint_print if $ma
	: $rt
	@_strprint_print
	<prc $ma>
	$a0++
	: strprint

%type strprint function
%size main strprint-main
%size strprint .-strprint
