#code
!fibonacci(50)
<print $r0>
<halt>

@fibonacci // $a0 -> $r0
[ $t0 $t1
	1 -> $t0
	1 -> $t1
	0 -> $r0
	@fibonacci_loop
		: fibonacci_loop_done if $a0 == $0; $a0--
		$t0 + $t1 -> $r0
		$t1 -> $t0
		$r0 -> $t1
		: fibonacci_loop
	@fibonacci_loop_done
] $t1 $t0; !ret
